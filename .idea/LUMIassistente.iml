<!-- ==========================================================
     ARQUIVO: module.iml
     DESCRIÇÃO: Arquivo de configuração do módulo Python no IntelliJ IDEA.
     FUNÇÃO: Define as pastas incluídas/excluídas do projeto e o interpretador (Python SDK) usado.
     ========================================================== -->

<?xml version="1.0" encoding="UTF-8"?> <!-- Indica que o arquivo está em formato XML e usa codificação UTF-8 -->
<module type="PYTHON_MODULE" version="4"> <!-- Define que este módulo é um projeto Python, na versão de esquema 4 -->
  <component name="NewModuleRootManager"> <!-- Gerencia o conteúdo e as dependências principais do módulo -->
    <content url="file://$MODULE_DIR$"> <!-- Especifica o diretório raiz do módulo dentro do projeto -->
      <excludeFolder url="file://$MODULE_DIR$/.venv" /> <!-- Exclui a pasta .venv (ambiente virtual) da indexação e análise do IDE -->
    </content> <!-- Fim do bloco que define o conteúdo do módulo -->
    <orderEntry type="jdk" jdkName="Python 3.13 (LUMI-assistente)" jdkType="Python SDK" /> <!-- Define o interpretador Python (SDK) que o módulo usa -->
    <orderEntry type="sourceFolder" forTests="false" /> <!-- Indica a pasta de código-fonte principal (não destinada a testes) -->
  </component> <!-- Encerra o gerenciador de conteúdo e dependências do módulo -->
</module> <!-- Fim do bloco principal do módulo Python -->
